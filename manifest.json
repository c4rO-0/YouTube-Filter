{
    "manifest_version": 2,
    "name": "__MSG_appName__",
    "description": "__MSG_appDesc__",
    "default_locale": "zh_CN",
    "version": "1.1.0",
    "browser_action": {
        "browser_style": true,
        "default_icon": {
            "16": "icons/c4r_16.png",
            "32": "icons/c4r_32.png",
            "64": "icons/c4r_64.png"
        },
        "default_title": "Youtube Filter",
        "default_popup":"popup/popup.html"
        
    },
    "background": {
        "scripts": [
            "lib/jquery-3.2.1.min.js",
            "lib/core.js",
            "background.js"
        ]
    },
    "icons": {
        "48": "icons/c4r_48.png",
        "96": "icons/c4r_96.png"
    },
    "options_ui": {
        "page": "settings/optionsV6.html",
        "open_in_tab": true
    },
    "permissions": [
        "notifications",
        "*://*.youtube.com/*",
        "webRequest",
        "webRequestBlocking",
        "<all_urls>",
        "storage",
        "tabs",
        "webNavigation",
        "downloads"
    ],
    "author": "c4r"
}